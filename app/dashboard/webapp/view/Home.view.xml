<mvc:View
    height="100%"
    controllerName="zdashboard.controller.Home"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:mc="sap.suite.ui.microchart"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <Page
        id="_IDGenPage1"
        showHeader="false"
    >
        <layout:BlockLayout
            id="_IDGenBlockLayout1"
            background="Light"
        >
            

            <layout:BlockLayoutRow id="_IDGenBlockLayoutRow1">
                <layout:BlockLayoutCell
                    id="_IDGenBlockLayoutCell1"
                    width="-1"
                >
                    <core:Icon
                        id="_IDGenIcon"
                        color="Default"
                        src="sap-icon://course-program"
                    />
                </layout:BlockLayoutCell>
                <layout:BlockLayoutCell
                    id="_IDGenBlockLayoutCell2"
                    titleLevel="H1"
                    width="100%"
                >
                    <layout:titleLink>
                        <Link
                            id="_IDGenLink2"
                            text="Invoices"
                            press=".onLinkPress"
                            customData:key="files"
                        />
                    </layout:titleLink>
                    <layout:content>
                        <Text
                            id="_IDGenText7"
                            text="File configurations for transmission to Msitek"
                        />
                    </layout:content>
                </layout:BlockLayoutCell>
            </layout:BlockLayoutRow>

            <layout:BlockLayoutRow id="_IDGenBlockLayoutRow2">
                <layout:BlockLayoutCell
                    id="_IDGenBlockLayoutCell3"
                    width="-1"
                >
                    <core:Icon
                        id="_IDGenIcon1"
                        color="Default"
                        src="sap-icon://role"
                    />
                </layout:BlockLayoutCell>

                <layout:BlockLayoutCell id="_IDGenBlockLayoutCell4">
                    <layout:titleLink>
                        <Link
                            id="_IDGenLink3"
                            text="Contacts"
                            press=".onLinkPress"
                            customData:key="contacts"
                        />
                    </layout:titleLink>
                    <layout:VerticalLayout id="_IDGenVerticalLayout">
                        <Text
                            id="_IDGenText8"
                            text="Contact Registration"
                        />
                    </layout:VerticalLayout>
                </layout:BlockLayoutCell>

                <layout:BlockLayoutCell id="_IDGenBlockLayoutCell5">
                    <layout:titleLink>
                        <Link
                            id="_IDGenLink4"
                            text="Segments"
                            press=".onLinkPress"
                            customData:key="segments"
                        />
                    </layout:titleLink>
                    <layout:content>
                        <Text
                            id="_IDGenText9"
                            text="Segment Registration"
                        />
                    </layout:content>
                </layout:BlockLayoutCell>
            </layout:BlockLayoutRow>
        </layout:BlockLayout>

        <layout:BlockLayout
            id="_IDGenBlockLayout2"
            class="elementAtBottom"
            background="Light"
        >
           
            <layout:BlockLayoutRow
			rowColorSet="ColorSet2">
			<layout:BlockLayoutCell>
				<Title
					text="Invoices fetched"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>orderStatusDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
					<mc:ComparisonMicroChart
						width="12rem"
						height="8rem">
						<mc:data>
							<mc:ComparisonMicroChartData
								color="Good"
								title="{i18n>initCalls}"
								displayValue="91"
								value="91" />
							<mc:ComparisonMicroChartData
								color="Error"
								title="{downtime}"
								displayValue="-46"
								value="-46" />
							<mc:ComparisonMicroChartData
								color="Critical"
								title="{maintenance}"
								displayValue="13"
								value="6" />
							<mc:ComparisonMicroChartData
									color="Good"
									title="{i18n>reLoads}"
									displayValue="6"
									value="6" />
							<mc:ComparisonMicroChartData
									color="Error"
									title="{i18n>maxMemUsage}"
									displayValue="18"
									value="18" />
						</mc:data>
					</mc:ComparisonMicroChart>
			</layout:BlockLayoutCell>
			<layout:BlockLayoutCell>
				<Title
					text="Invoices extracted"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>customerRatingDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
					<mc:ColumnMicroChart
						height="8rem"
						width="12rem">
						<mc:columns>
							<mc:ColumnMicroChartData
								value="10"
								color="Good" />
							<mc:ColumnMicroChartData
								value="2"
								color="Error" />
							<mc:ColumnMicroChartData
								value="4"
								color="Critical" />
						</mc:columns>
					</mc:ColumnMicroChart>
			</layout:BlockLayoutCell>
			<layout:BlockLayoutCell>
				<Title
					text="Invoices manually created"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>salesTrendDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
				<mc:AreaMicroChart
						width="20rem"
						height="8rem"
						minXValue="0"
						maxXValue="100">
					<mc:firstXLabel>
						<mc:AreaMicroChartLabel
							label="{i18n>salesTrendLabelXStart}"
							color="Error" />
					</mc:firstXLabel>
					<mc:lastXLabel>
						<mc:AreaMicroChartLabel
							label="{i18n>salesTrendLabelXEnd}"
							color="Good" />
					</mc:lastXLabel>
					<mc:firstYLabel>
						<mc:AreaMicroChartLabel
							label="{i18n>salesTrendLabelYStart}"
							color="Error" />
					</mc:firstYLabel>
					<mc:lastYLabel>
						<mc:AreaMicroChartLabel
							label="{i18n>salesTrendLabelYEnd}"
							color="Good"/>
					</mc:lastYLabel>
					<mc:chart>
						<mc:AreaMicroChartItem>
							<mc:points>
								<mc:AreaMicroChartPoint x="0" y="0" />
								<mc:AreaMicroChartPoint x="20" y="31" />
								<mc:AreaMicroChartPoint x="36" y="27" />
								<mc:AreaMicroChartPoint x="48" y="55" />
								<mc:AreaMicroChartPoint x="91" y="81" />
							</mc:points>
						</mc:AreaMicroChartItem>
					</mc:chart>
					<mc:target>
						<mc:AreaMicroChartItem>
							<mc:points>
								<mc:AreaMicroChartPoint x="0" y="0" />
								<mc:AreaMicroChartPoint x="30" y="25" />
								<mc:AreaMicroChartPoint x="55" y="64" />
								<mc:AreaMicroChartPoint x="67" y="68" />
								<mc:AreaMicroChartPoint x="81" y="89" />
							</mc:points>
						</mc:AreaMicroChartItem>
					</mc:target>
					<mc:maxThreshold>
						<mc:AreaMicroChartItem color="Good">
							<mc:points>
								<mc:AreaMicroChartPoint x="0" y="0" />
								<mc:AreaMicroChartPoint x="30" y="40" />
								<mc:AreaMicroChartPoint x="60" y="50" />
								<mc:AreaMicroChartPoint x="100" y="100" />
							</mc:points>
						</mc:AreaMicroChartItem>
					</mc:maxThreshold>
					<mc:minThreshold>
						<mc:AreaMicroChartItem color="Error">
							<mc:points>
								<mc:AreaMicroChartPoint x="0" y="0" />
								<mc:AreaMicroChartPoint x="30" y="20" />
								<mc:AreaMicroChartPoint x="60" y="30" />
								<mc:AreaMicroChartPoint x="100" y="70" />
							</mc:points>
						</mc:AreaMicroChartItem>
					</mc:minThreshold>
				</mc:AreaMicroChart>
			</layout:BlockLayoutCell>
			
		</layout:BlockLayoutRow>
		<layout:BlockLayoutRow
			rowColorSet="ColorSet1">
			<layout:BlockLayoutCell>
				<Title
					text="Invoices from Email"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>netPromoterDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
				<FlexBox
					height="8rem"
					width="12rem"
					direction="Column"
					renderType="Bare"
					alignItems="Center">
					<mc:BulletMicroChart
						targetValue="100">
						<mc:actual>
							<mc:BulletMicroChartData
								value="87"
								color="Good" />
						</mc:actual>
						<mc:thresholds>
							<mc:BulletMicroChartData
								value="0"
								color="Error" />
							<mc:BulletMicroChartData
								value="20"
								color="Error" />
							<mc:BulletMicroChartData
								value="21"
								color="Critical" />
							<mc:BulletMicroChartData
								value="49"
								color="Critical" />
							<mc:BulletMicroChartData value="75" color="Good" />
						</mc:thresholds>
					</mc:BulletMicroChart>
					<mc:BulletMicroChart
						targetValue="100">
						<mc:actual>
							<mc:BulletMicroChartData
								value="19"
								color="Error" />
						</mc:actual>
						<mc:thresholds>
							<mc:BulletMicroChartData
								value="0"
								color="Error" />
							<mc:BulletMicroChartData
								value="20"
								color="Error" />
							<mc:BulletMicroChartData
								value="21"
								color="Critical" />
							<mc:BulletMicroChartData
								value="49"
								color="Critical" />
							<mc:BulletMicroChartData
								value="75"
								color="Good" />
						</mc:thresholds>
					</mc:BulletMicroChart>
				</FlexBox>
			</layout:BlockLayoutCell>
			<layout:BlockLayoutCell>
				<Title
					text="DB usage"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>revenueDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
				<FlexBox
					alignItems="Center"
					width="12rem"
					height="8rem"
					renderType="Bare"
					direction="Column">
					<mc:HarveyBallMicroChart
						total="100"
						totalScale="Hz"
						showFractions="true">
						<mc:items>
							<mc:HarveyBallMicroChartItem
								fraction="56.5"
								color="Critical"
								fractionScale="Mrd" />
						</mc:items>
					</mc:HarveyBallMicroChart>
				</FlexBox>
			</layout:BlockLayoutCell>
			<layout:BlockLayoutCell>
				<Title
					text="{i18n>impCostCenter}"
					level="H4"
					titleStyle="H2"
					width="100%"/>
				<Title
					text="{i18n>goalProgressDate}"
					level="H5"
					width="100%"
					class="sapUiSmallMarginBottom"/>
				<FlexBox
					width="12rem"
					height="8rem"
					renderType="Bare">
					<items>
						<mc:RadialMicroChart
							percentage="78"
							valueColor="Good"/>
					</items>
				</FlexBox>
			</layout:BlockLayoutCell>
			
		</layout:BlockLayoutRow>
		 <layout:BlockLayoutRow id="_IDGenBlockLayoutRow3">
                <layout:BlockLayoutCell
                    id="_IDGenBlockLayoutCell6"
                    titleAlignment="Center"
                    title="Questions?"
                >
                    <HBox
                        id="_IDGenHBox1"
                        justifyContent="Center"
                    >
                        <FormattedText
                            id="_IDGenFormattedText"
                            htmlText="{i18n>duvidas}"
                        />
                    </HBox>
                </layout:BlockLayoutCell>
            </layout:BlockLayoutRow>
        </layout:BlockLayout>
    </Page>
</mvc:View>
